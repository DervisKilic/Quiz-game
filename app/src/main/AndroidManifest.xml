<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gualbertoscolari.grupp3">
    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.SplashActivity"
            android:label="@string/Quiz">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.ResultActivity" />
        <activity android:name=".Activity.HighscoreActivity" />
        <activity android:name=".Activity.MainGameActivity" />
        <activity android:name=".Activity.GameSettingsActivity" />
        <activity android:name=".CreateProfileActivity" />
        <activity android:name=".CreateCategoryActivity" />
        <activity android:name=".CreateQuestionActivity" />
        <activity android:name=".Activity.AboutActivity" />
        <activity android:name=".Activity.MenuActivity" />
        <activity android:name=".Activity.ManageContentActivity"></activity>
    </application>

</manifest>